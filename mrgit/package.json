{
    "name": "mrgit",
    "displayName": "mrgit",
    "description": "right click menu for git",
    "version": "0.0.1",
    "publisher": "salilvnair",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello1",
                "title": "Init Hello"
            },
            {
                "command": "extension.track",
                "title": "Git-Track"
            },
            {
                "command": "extension.untrack",
                "title": "Git-Untrack"
            },
            {
                "command": "extension.untrackList",
                "title": "Show Git-Untrack List"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "extension.track",
                    "group": "navigation"
                },
                {
                    "command": "extension.untrack",
                    "group": "navigation"
                },
                {
                    "command": "extension.untrackList",
                    "group": "navigation"
                }
            ],
            "explorer/context": [
                {
                    "command": "extension.track",
                    "group": "navigation"
                },
                {
                    "command": "extension.untrack",
                    "group": "navigation"
                },
                {
                    "command": "extension.untrackList",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
